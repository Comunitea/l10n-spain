<odoo>
    <!--  CONFIG MOD 182  -->
    <record id="aeat_mod182_export_partner_donation_config" model="aeat.model.export.config">
        <field name="name">Modelo 182:  Registro 2</field>
        <field name="model_number">182</field>
        <!-- <field name="model_id" ref="model_l10n_es_aeat_mod182_report" /> -->
        <field name="active" eval="True" />
    </record>

    <!-- (1) TIPO REGISTRO 2 (1) -->
    <record id="aeat_mod182_partner_donation_export_line_01" model="aeat.model.export.config.line">
        <field name="sequence">1</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config"/>
        <field name="name">Tipo Registro 2</field>
        <field name="fixed_value">2</field>
        <field name="export_type">integer</field>
        <field name="size">1</field>
        <field name="alignment">left</field>
    </record>
     <!-- (2-4) MODELO DECLARACIÓN (3) -->
    <record id="aeat_mod182_partner_donation_export_line_02" model="aeat.model.export.config.line">
        <field name="sequence">2</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config"/>
        <field name="name">Modelo Declaración</field>
        <field name="fixed_value">182</field>
        <field name="export_type">integer</field>
        <field name="size">3</field>
        <field name="alignment">left</field>
    </record>

    <!-- (5-8) Ejercicio (4) -->
    <record id="aeat_mod182_partner_donation_line_03" model="aeat.model.export.config.line">
        <field name="sequence">3</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config"/>
        <field name="name">Ejercicio</field>
        <field name="expression">${object.report_id.year}</field>
        <field name="export_type">string</field>
        <field name="size">4</field>
        <field name="alignment">left</field>
    </record>

    <!-- (9-17) NIF DECLARANTE (9) -->
    <record id="aeat_mod182_partner_donation_line_04" model="aeat.model.export.config.line">
        <field name="sequence">4</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config"/>
        <field name="name">NIF DEL DECLARANTE</field>
        <!-- <field name="expression">${object.report_id.company_vat_mod182}</field> -->
        <field name="expression">${object.report_id.company_vat}</field>
        <field name="export_type">string</field>
        <field name="size">9</field>
        <field name="alignment">right</field>
    </record>
    <!-- (18-26) NIF DECLARADO (9) -->
    <record id="aeat_mod182_partner_donation_line_05" model="aeat.model.export.config.line">
        <field name="sequence">5</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config"/>
        <field name="name">NIF DEL DECLARADO</field>
        <field name="expression">${object.nif_declarado}</field>
        <field name="export_type">string</field>
        <field name="size">9</field>
        <field name="alignment">right</field>
    </record>
    <!-- (27-35) NIF representante legal (9) -->
    <record id="aeat_mod182_partner_donation_line_06" model="aeat.model.export.config.line">
        <field name="sequence">6</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config"/>
        <field name="name">NIF REPRESENTANTE LEGAL</field>
        <field name="expression">${object.nif_representante}</field>
        <field name="export_type">string</field>
        <field name="size">9</field>
        <field name="alignment">left</field>
    </record>
    <!-- (36-75) Razon Social Declarado (40) -->
    <record id="aeat_mod182_partner_donation_line_07" model="aeat.model.export.config.line">
        <field name="sequence">7</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config"/>
        <field name="name">Razon Social Declarado</field>
        <field name="expression">${object.partner_id.name}</field>
        <field name="export_type">string</field>
        <field name="size">40</field>
        <field name="alignment">left</field>
    </record>
    <!-- (76-77) Código provincia (40) -->
    <record id="aeat_mod182_partner_donation_line_08" model="aeat.model.export.config.line">
        <field name="sequence">8</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config"/>
        <field name="name">Código provincia</field>
        <!-- <field name="expression">${object.partner_id.state_id.code}</field> -->
        <field name="expression">${object.codigo_provincia}</field>
        <field name="export_type">string</field>
        <field name="size">2</field>
        <field name="alignment">left</field>
    </record>
    <!-- (78) CLAVE (40) -->
    <!-- ? es algo del partner o de los donativos -->
    <record id="aeat_mod182_partner_donation_line_09" model="aeat.model.export.config.line">
        <field name="sequence">09</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config"/>
        <field name="name">CLAVE</field>
        <field name="expression">${object.clave_mod182}</field>
        <field name="export_type">string</field>
        <field name="size">1</field>
        <field name="alignment">left</field>
    </record>
    <!--> (79-83) % DEDUCCIÓN -->
    <!-- 79-81 Parte entera -->
    <!-- 82-83 Parte decimal -->
    <record id="aeat_mod182_partner_donation_line_10" model="aeat.model.export.config.line">
        <field name="sequence">10</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config"/>
        <field name="name">% DEDUCCIÓN</field>
        <field name="expression">${object.deduccion_mod182}</field>
        <field name="export_type">float</field>
        <field name="apply_sign" eval="False" />
        <field name="size">5</field>
        <field name="decimal_size">2</field>
        <field name="alignment">right</field>
    </record>
    <!--> (84-96) IMPORTE O VALORACIÓN DEL DONATIVO  -->
    <!-- 84-94 Parte entera -->
    <!-- 95-96 Parte decimal -->
    <record id="aeat_mod182_partner_donation_line_11" model="aeat.model.export.config.line">
        <field name="sequence">11</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config"/>
        <field name="name">% DEDUCCIÓN</field>
        <field name="expression">${object.amount}</field>
        <field name="export_type">float</field>
        <field name="apply_sign" eval="False" />
        <field name="size">13</field>
        <field name="decimal_size">2</field>
        <field name="alignment">right</field>
    </record>
    <!-- (97) DONATIVO APORTACION O DISPOSICIÓN EN ESPECIE  (1)-->
    <record id="aeat_mod182_partner_donation_line_12" model="aeat.model.export.config.line">
        <field name="sequence">12</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config" />
        <field name="name">DONATIVO APORTACION O DISPOSICIÓN EN ESPECIE</field>
        <field name="expression">${object.aeat_especie == True}</field>
        <field name="export_type">boolean</field>
        <field name="size">1</field>
        <field name="bool_yes">X</field>
        <field name="bool_no"> </field>
        <field name="alignment">left</field>
    </record>
    <!-- (98-99) DEDUCCIÓN COM. AUTÓNOMA (2) -->
    <record id="aeat_mod182_partner_donation_line_13" model="aeat.model.export.config.line">
        <field name="sequence">13</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config"/>
        <field name="name">Razon Social Declarado</field>
        <field name="expression">${object.codigo_ccaa}</field>
        <field name="export_type">string</field>
        <field name="size">2</field>
        <field name="alignment">left</field>
    </record>
    <!-- (100-104) % DE DEDUCCIÓN COM. AUTÓNOMA.-->
     <record id="aeat_mod182_partner_donation_line_14" model="aeat.model.export.config.line">
        <field name="sequence">14</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config"/>
        <field name="name">% DE DEDUCCIÓN COM. AUTÓNOMA.</field>
        <field name="expression">${object.deduccion_ccaa}</field>
        <field name="export_type">float</field>
        <field name="apply_sign" eval="False" />
        <field name="size">5</field>
        <field name="decimal_size">2</field>
        <field name="alignment">right</field>
    </record>
    <!-- (105) NATURALEZA DEL DECLARADO -->
    <record id="aeat_mod182_partner_donation_line_15" model="aeat.model.export.config.line">
        <field name="sequence">15</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config"/>
        <field name="name">NATURALEZA DEL DECLARADO</field>
        <field name="expression">${object.naturaleza_declarado}</field>
        <field name="export_type">string</field>
        <field name="size">1</field>
        <field name="alignment">left</field>
    </record>
    <!-- (106) REVOCACIÓN -->
    <record id="aeat_mod182_partner_donation_line_16" model="aeat.model.export.config.line">
        <field name="sequence">16</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config"/>
        <field name="name">REVOCACIÓN</field>
        <field name="expression">${object.revocacion == True}</field>
        <field name="export_type">boolean</field>
        <field name="size">1</field>
        <field name="bool_yes">X</field>
        <field name="bool_no"> </field>
        <field name="alignment">left</field>
    </record>
    <!-- (107-110) EJERCICIO REVOCACIÓN (4) -->
    <record id="aeat_mod182_partner_donation_line_17" model="aeat.model.export.config.line">
        <field name="sequence">17</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config"/>
        <field name="name">EJERCICIO REVOCACIÓN</field>
        <field name="expression">${object.ejerecicio_revocacion}</field>
        <field name="export_type">integer</field>
        <field name="size">4</field>
        <field name="alignment">left</field>
    </record>
    <!-- (111) TIPO DE BIEN  -->
    <record id="aeat_mod182_partner_donation_line_18" model="aeat.model.export.config.line">
        <field name="sequence">18</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config"/>
        <field name="name">TIPO DE BIEN </field>
        <field name="expression">${object.tipo_bien}</field>
        <field name="export_type">string</field>
        <field name="size">1</field>
        <field name="alignment">left</field>
    </record>
    <!-- (112-131) DESCRIPCIÓN DEL BIEN (20) -->
    <record id="aeat_mod182_partner_donation_line_19" model="aeat.model.export.config.line">
        <field name="sequence">19</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config"/>
        <field name="name">EJERCICIO EN QUE SE EFECTUÓ LA DONACIÓN</field>
        <field name="expression">${object.identificacion_bien}</field>
        <field name="export_type">string</field>
        <field name="size">20</field>
        <field name="alignment">left</field>
    </record>
    <!-- (132) RECURRENCIA DONATIVOS  -->
    <record id="aeat_mod182_partner_donation_line_20" model="aeat.model.export.config.line">
        <field name="sequence">20</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config"/>
        <field name="name">RECURRENCIA DONATIVOS</field>
        <field name="expression">${object.recurrencia == True}</field>
        <field name="export_type">boolean</field>
        <field name="size">1</field>
        <field name="bool_yes">1</field>
        <field name="bool_no">2</field>
        <field name="alignment">left</field>
    </record>
    <!-- (133-141) NIF TITULAR PATRIMONIO PROTEGIDO (9) -->
    <record id="aeat_mod182_partner_donation_line_21" model="aeat.model.export.config.line">
        <field name="sequence">21</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config"/>
        <field name="name">NIF DECLARANTE</field>
        <field name="expression">${object.nif_patrimonio_protegido}</field>
        <field name="export_type">string</field>
        <field name="size">9</field>
        <field name="alignment">left</field>
    </record>
    <!-- (142-181) NOMBRE TITULAR PATRIMONIO PROTEGIDO (40) -->
    <record id="aeat_mod182_partner_donation_line_22" model="aeat.model.export.config.line">
        <field name="sequence">22</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config"/>
        <field name="name"> NOMBRE TITULAR PATRIMONIO PROTEGIDO</field>
        <field name="expression">${object.nombre_patrimonio_protegido}</field>
        <field name="export_type">string</field>
        <field name="size">40</field>
        <field name="alignment">left</field>
    </record>
     <!-- (182-250) BLANCOS (69) -->
    <record id="aeat_mod182_partner_donation_line_23" model="aeat.model.export.config.line">
        <field name="sequence">23</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config" />
        <field name="name">Reservado para la Administración. Rellenar con blancos</field>
        <field name="fixed_value" />
        <field name="export_type">string</field>
        <field name="size">69</field>
        <field name="alignment">left</field>
    </record>
    <!-- FIN DE REGISTRO -->
    <!-- (0) NUEVA LINEA -->
    <record id="aeat_mod182_partner_donation_line_24" model="aeat.model.export.config.line">
        <field name="sequence">24</field>
        <field name="export_config_id" ref="aeat_mod182_export_partner_donation_config" />
        <field name="name">Cambio línea: Constante CRLF (Hexadecimal 0D0A, Decimal 1310)</field>
        <field name="expression">${"\r\n".encode("ascii")}</field>
        <field name="export_type">string</field>
        <field name="size">2</field>
        <field name="alignment">left</field>
    </record>

</odoo>
